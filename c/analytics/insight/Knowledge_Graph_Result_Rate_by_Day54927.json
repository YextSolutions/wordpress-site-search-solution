{
  "$id": "c_Knowledge_Graph_Result_Rate_by_Day54927",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Knowledge Graph Result Rate by Day",
  "description": "Knowledge Graph Result Rate by Day",
  "query": {
    "metrics": [
      "yext/ANSWERS_KG_RESULT_RATE"
    ],
    "dimensions": [
      "yext/DAYS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_30"
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "SUM"
    }
  }
}