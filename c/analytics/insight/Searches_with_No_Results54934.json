{
  "$id": "c_Searches_with_No_Results54934",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Searches with No Results",
  "description": "Answers Searches with Knowledge Graph Results by Entity",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES_WITH_KG_RESULTS"
    ],
    "fields": [
      "entityId",
      "name",
      "address",
      "yext/derivedField-postalCode",
      "yext/derivedField-city"
    ],
    "dimensions": [
      "yext/ENTITIES"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_30"
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_SEARCHES_WITH_KG_RESULTS"
        }
      }
    }
  }
}